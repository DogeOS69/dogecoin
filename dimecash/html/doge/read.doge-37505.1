read.doge () 
{ 
    TARGET=$(dc.setname doge );
    TARGET=/var/www/html/doge/$1;
    ( grep " DCx" $TARGET | while read LINE; do
        echo $LINE | grep zzz;
    done;
    grep " DCy" $TARGET | while read LINE; do
        echo $LINE | grep zzz;
    done;
    grep " DCz" $TARGET | while read LINE; do
        echo $LINE | grep zzz;
    done;
    grep ".00039692" $TARGET;
    grep " 9sDC" $TARGET;
    grep "DCULT" $TARGET ) | sort -u
}
