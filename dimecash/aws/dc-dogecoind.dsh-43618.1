dsh.bootstrap () 
{ 
    SSH="/usr/bin/ssh -i ./id_rsa -n -oStrictHostKeyChecking=no ";
    let COUNT=0;
    USER=ubuntu;
    aws.public_ips | while read ADDR; do
        $SSH $USER@$ADDR exit;
        scp -i ./id_rsa bootstrap.bash $USER@$ADDR:.;
        $SSH $USER@$ADDR "bash ./bootstrap.bash $COUNT";
        let COUNT=COUNT+1;
    done
} 
