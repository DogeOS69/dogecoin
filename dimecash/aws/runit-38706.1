runit () 
{ 
    cd us-east-1;
    : terraform apply -auto-approve;
    : aws.ansible-hosts dogecoind > ../hosts;
    cd ..;
    : ansible-playbook dogecoind.yml;
    aws.public-ips | while read IP; do
        scp -i ./id_rsa bootstrap2.bash ubuntu@$IP:.;
        scp -i ./id_rsa id_rsa ubuntu@$IP:.ssh;
        ssh -i ./id_rsa ubuntu@$IP "bash ./bootstrap2.bash";
    done
}
